// Star Pattern(Half Pyramid)

//*
//* *
//* * *
//* * * *
//* * * * *

#include <stdio.h>

int main() {
   int n;
   scanf("%d", &n); // enter the no. of rows
   for (int i = 1; i <= n; ++i) // to print no. of rows
   {
        for (int j = 1; j <= i; ++j) // to print column
        {
            printf("* ");
        }
        printf("\n");
   }
   return 0;
}



